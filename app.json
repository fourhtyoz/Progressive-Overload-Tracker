{
  "expo": {
    "name": "Progressive Overload Tracker",
    "slug": "progressive_overload_tracker",
    "version": "1.0.0",
    "icon": "./src/icon.png",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fourhtyoz.progressive_overload_tracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "38b99c0a-6c65-4a5d-a2e1-a37b9b959659"
      }
    }
  }
}
